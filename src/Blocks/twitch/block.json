{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "title": "Twitch",
  "name": "embedpress/twitch-block",
  "category": "embedpress",
  "description": "Embed Twitch videos, streams, and clips with customizable player options and responsive sizing controls.",
  "apiVersion": 2,
  "textdomain": "embedpress",
  "editorScript": "embedpress-blocks-editor",
  "editorStyle": "embedpress-blocks-editor-style",
  "style": "embedpress-blocks-style",
  "supports": {
    "align": ["left", "center", "right", "wide", "full"],
    "html": false,
    "anchor": true,
    "lightBlockWrapper": true,
    "default": "center"
  }
}
