{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "title": "Google Maps",
  "name": "embedpress/google-maps-block",
  "category": "embedpress",
  "description": "Embed Google Maps with interactive location features, custom markers, and responsive map controls for enhanced user experience.",
  "apiVersion": 2,
  "textdomain": "embedpress",
  "editorScript": "embedpress-blocks-editor",
  "editorStyle": "embedpress-blocks-editor-style",
  "style": "embedpress-blocks-style",
  "supports": {
    "align": ["left", "center", "right", "wide", "full"],
    "html": false,
    "anchor": true,
    "lightBlockWrapper": true,
    "default": "center"
  }
}
