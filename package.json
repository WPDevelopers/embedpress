{
  "name": "embedpress",
  "version": "4.2.7",
  "description": "EmbedPress lets you embed videos, images, posts, audio, maps and upload PDF, DOC, PPT & all other types of content into your WordPress site with one-click and showcase it beautifully for the visitors.",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npm run dev:blocks",
    "build": "npm run build:all",
    "preview": "vite preview",
    "dev:blocks": "BUILD_TARGET=blocks vite",
    "dev:admin": "BUILD_TARGET=admin vite",
    "dev:frontend": "BUILD_TARGET=frontend vite",
    "build:blocks": "BUILD_TARGET=blocks vite build && node scripts/split-css.js blocks",
    "build:admin": "BUILD_TARGET=admin vite build",
    "build:frontend": "BUILD_TARGET=frontend vite build",
    "build:all": "npm run build:blocks && npm run build:admin && npm run build:frontend",

    "build:assets:legacy-css": "ASSET_BUILD_TYPE=legacyCSS vite build --config vite.assets.config.js",
    "build:assets:legacy-js": "ASSET_BUILD_TYPE=legacyJS vite build --config vite.assets.config.js",
    "build:assets:vendor": "ASSET_BUILD_TYPE=vendor vite build --config vite.assets.config.js",
    "build:assets:bundles": "ASSET_BUILD_TYPE=bundles vite build --config vite.assets.config.js",
    "build:assets:all": "npm run build:assets:legacy-css && npm run build:assets:legacy-js && npm run build:assets:vendor && npm run build:assets:bundles",

    "assets:scan": "node scripts/asset-manager.js scan",
    "assets:bundles": "node scripts/asset-manager.js bundles",
    "assets:third-party": "node scripts/asset-manager.js third-party",
    "assets:report": "node scripts/asset-manager.js report",
    "assets:manage": "node scripts/asset-manager.js all",

    "css:scan": "node scripts/asset-manager.js scan",
    "css:bundles": "node scripts/asset-manager.js bundles",
    "css:third-party": "node scripts/asset-manager.js third-party",
    "css:report": "node scripts/asset-manager.js report",
    "css:manage": "node scripts/asset-manager.js all",
    "watch:blocks": "BUILD_TARGET=blocks vite build --watch --mode development",
    "watch:admin": "BUILD_TARGET=admin vite build --watch --mode development",
    "watch:frontend": "BUILD_TARGET=frontend vite build --watch --mode development",
    "watch:all": "concurrently \"npm run watch:blocks\" \"npm run watch:admin\" \"npm run watch:frontend\"",
    "start": "npm run watch:all",
    "start:blocks": "npm run watch:blocks",
    "start:admin": "npm run watch:admin",
    "start:frontend": "npm run watch:frontend",
    "start:with-css": "concurrently \"npm run watch:all\" \"npm run css:watch\"",

    "css:build": "node scripts/css-builder.js",
    "css:watch": "node scripts/css-builder.js --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "pot": "wp i18n make-pot . languages/embedpress.pot",
    "clean": "rimraf assets/js assets/css",
    "clean:legacy": "rimraf Gutenberg/dist Gutenberg/node_modules analytics-dashboard/dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WPDevelopers/embedpress.git"
  },
  "keywords": [
    "embed",
    "youtube",
    "gutenberg",
    "pdf",
    "docs",
    "elementor",
    "video",
    "google-docs",
    "maps",
    "content",
    "iframes",
    "vimeo",
    "wistia",
    "player",
    "block-editor",
    "wordpress",
    "wpml",
    "social-media"
  ],
  "author": "EmbedPress, asif2bd, re_enter_rupok, wpdevteam, seakashdiu, alimuzzamanalim",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/WPDevelopers/embedpress/issues"
  },
  "homepage": "https://github.com/WPDevelopers/embedpress#readme",
  "dependencies": {
    "@wordpress/api-fetch": "^6.44.0",
    "@wordpress/block-editor": "^12.15.0",
    "@wordpress/blocks": "^12.24.0",
    "@wordpress/components": "^25.13.0",
    "@wordpress/compose": "^6.24.0",
    "@wordpress/core-data": "^6.24.0",
    "@wordpress/data": "^9.17.0",
    "@wordpress/edit-post": "^7.24.0",
    "@wordpress/element": "^5.24.0",
    "@wordpress/hooks": "^3.47.0",
    "@wordpress/html-entities": "^3.47.0",
    "@wordpress/i18n": "^4.47.0",
    "@wordpress/keycodes": "^3.47.0",
    "@wordpress/notices": "^4.15.0",
    "@wordpress/plugins": "^6.15.0",
    "@wordpress/primitives": "^3.45.0",
    "@wordpress/rich-text": "^6.24.0",
    "@wordpress/server-side-render": "^4.24.0",
    "@wordpress/url": "^3.48.0",
    "@wordpress/viewport": "^5.24.0",
    "axios": "^1.7.0",
    "chart.js": "^4.4.0",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "md5": "^2.3.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.0",
    "react-select": "^5.9.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/ui": "^1.6.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^5.0.0",
    "@wordpress/scripts": "^27.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.0",
    "sass": "^1.77.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0",
    "vite": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
