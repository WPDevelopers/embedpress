{
  "name": "embedpress",
  "version": "4.2.7",
  "description": "EmbedPress lets you embed videos, images, posts, audio, maps and upload PDF, DOC, PPT & all other types of content into your WordPress site with one-click and showcase it beautifully for the visitors.",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:blocks && npm run build:admin && npm run build:analytics && npm run build:frontend && npm run build:static",
    "build:blocks": "BUILD_TARGET=blocks vite build",
    "build:admin": "BUILD_TARGET=admin vite build",
    "build:analytics": "BUILD_TARGET=analytics vite build",
    "build:frontend": "BUILD_TARGET=frontend vite build",
    "build:static": "npm run build:admin-common && npm run build:settings && npm run build:preview && npm run build:common && npm run build:initplyr && npm run build:gallery && npm run build:elementor && npm run build:document-viewer && npm run build:gutenberg && npm run build:ads",
    "build:admin-common": "BUILD_TARGET=admin-common vite build",
    "build:initplyr": "BUILD_TARGET=initplyr vite build",
    "build:preview": "BUILD_TARGET=preview vite build",
    "build:common": "BUILD_TARGET=common vite build",
    "build:gallery": "BUILD_TARGET=gallery vite build",
    "build:elementor": "BUILD_TARGET=elementor vite build",
    "build:document-viewer": "BUILD_TARGET=document-viewer vite build",
    "build:gutenberg": "BUILD_TARGET=gutenberg vite build",
    "build:ads": "BUILD_TARGET=ads vite build",
    "build:settings": "BUILD_TARGET=settings vite build",
    "start": "npm run build && npm run watch",
    "watch": "npm run watch:blocks & npm run watch:admin & npm run watch:analytics & npm run watch:frontend & npm run watch:static",
    "watch:blocks": "BUILD_TARGET=blocks vite build --watch --mode development",
    "watch:admin": "BUILD_TARGET=admin vite build --watch --mode development",
    "watch:analytics": "BUILD_TARGET=analytics vite build --watch --mode development",
    "watch:frontend": "BUILD_TARGET=frontend vite build --watch --mode development",
    "watch:static": "npm run watch:admin-common & npm run watch:settings & npm run watch:preview & npm run watch:common & npm run watch:initplyr & npm run watch:gallery & npm run watch:elementor & npm run watch:document-viewer & npm run watch:gutenberg & npm run watch:ads",
    "watch:admin-common": "BUILD_TARGET=admin-common vite build --watch --mode development",
    "watch:initplyr": "BUILD_TARGET=initplyr vite build --watch --mode development",
    "watch:preview": "BUILD_TARGET=preview vite build --watch --mode development",
    "watch:common": "BUILD_TARGET=common vite build --watch --mode development",
    "watch:gallery": "BUILD_TARGET=gallery vite build --watch --mode development",
    "watch:elementor": "BUILD_TARGET=elementor vite build --watch --mode development",
    "watch:document-viewer": "BUILD_TARGET=document-viewer vite build --watch --mode development",
    "watch:gutenberg": "BUILD_TARGET=gutenberg vite build --watch --mode development",
    "watch:ads": "BUILD_TARGET=ads vite build --watch --mode development",
    "watch:settings": "BUILD_TARGET=settings vite build --watch --mode development",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "pot": "wp i18n make-pot . languages/embedpress.pot",
    "clean": "rimraf assets/js assets/css",
    "clean:legacy": "rimraf Gutenberg/dist Gutenberg/node_modules analytics-dashboard/dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WPDevelopers/embedpress.git"
  },
  "keywords": [
    "embed",
    "youtube",
    "gutenberg",
    "pdf",
    "docs",
    "elementor",
    "video",
    "google-docs",
    "maps",
    "content",
    "iframes",
    "vimeo",
    "wistia",
    "player",
    "block-editor",
    "wordpress",
    "wpml",
    "social-media"
  ],
  "author": "EmbedPress, asif2bd, re_enter_rupok, wpdevteam, seakashdiu, alimuzzamanalim",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/WPDevelopers/embedpress/issues"
  },
  "homepage": "https://github.com/WPDevelopers/embedpress#readme",
  "dependencies": {
    "@amcharts/amcharts5": "^5.13.3",
    "@wordpress/api-fetch": "^6.44.0",
    "@wordpress/block-editor": "^12.15.0",
    "@wordpress/blocks": "^12.24.0",
    "@wordpress/components": "^25.13.0",
    "@wordpress/compose": "^6.24.0",
    "@wordpress/core-data": "^6.24.0",
    "@wordpress/data": "^9.17.0",
    "@wordpress/edit-post": "^7.24.0",
    "@wordpress/element": "^5.24.0",
    "@wordpress/hooks": "^3.47.0",
    "@wordpress/html-entities": "^3.47.0",
    "@wordpress/i18n": "^4.47.0",
    "@wordpress/keycodes": "^3.47.0",
    "@wordpress/notices": "^4.15.0",
    "@wordpress/plugins": "^6.15.0",
    "@wordpress/primitives": "^3.45.0",
    "@wordpress/rich-text": "^6.24.0",
    "@wordpress/server-side-render": "^4.24.0",
    "@wordpress/url": "^3.48.0",
    "@wordpress/viewport": "^5.24.0",
    "axios": "^1.7.0",
    "chart.js": "^4.4.0",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "md5": "^2.3.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.0",
    "react-select": "^5.9.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/ui": "^1.6.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^5.0.0",
    "@wordpress/scripts": "^27.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.0",
    "sass": "^1.77.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0",
    "vite": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
